all:
	mkdir output
	g++ -std=c++17 -I../ test_default_constructor.cpp -o ./output/test_default_constructor
	g++ -std=c++17 -I../ test_copy_constructor.cpp -o ./output/test_copy_constructor
	g++ -std=c++17 -I../ test_assignmet_operator.cpp -o ./output/test_assignmet_operator
	g++ -std=c++17 -I../ test_constructor_initializer_list.cpp -o ./output/test_constructor_initializer_list
	g++ -std=c++17 -I../ test_size.cpp -o ./output/test_size
        
clean:
	rm -rf ./output
